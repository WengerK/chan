{
  "name": "chan",
  "version": "1.0.0",
  "description": "Chan is a likeable CLI tool used for writing and maintaining a CHANGELOG empowering the user to use a coloquial/friendly style. See more here: http://keepachangelog.com/",
  "main": "./es5/index.js",
  "bin": {
    "chan": "./es5/cli/index.js"
  },
  "scripts": {
    "test": "tape -r babel-register \"test/**/*.js\" | tap-spec",
    "posttest": "eslint src",
    "build": "del-cli es5 && babel src --out-dir es5",
    "watch": "npm run build -- --watch"
  },
  "files": [
    "es5"
  ],
  "dependencies": {
    "commander": "^2.9.0",
    "remark": "^4.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "del-cli": "^0.2.0",
    "eslint": "^2.6.0",
    "eslint-config-postcss": "^2.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tempfile": "^1.1.1"
  },
  "eslintConfig": {
    "extends": "postcss",
    "rules": {
      "max-len": 0
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign",
      "add-module-exports"
    ]
  },
  "keywords": [
    "cli",
    "geut",
    "tooling",
    "changelog"
  ],
  "author": "GEUT",
  "license": "ISC"
}
